@page
@model IndexModel
@{
    ViewData["Title"] = "Cincinnati Accidents Portal";
}
<link rel="stylesheet" type="text/css" href="~/css/StyleSheet.css" />
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
<body>
    <div>
        <div id="myCarousel" class="carousel slide" data-ride="carousel">
            <!-- Indicators -->
            <ol class="carousel-indicators">
                <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                <li data-target="#myCarousel" data-slide-to="1"></li>
                <li data-target="#myCarousel" data-slide-to="2"></li>
            </ol>

            <!-- Wrapper for slides -->
            <div class="carousel-inner">
                <div class="item active">
                    <img src="https://s3-prod.chicagobusiness.com/gettyimages-chicago-skyline-low.jpg" alt="img1" width="500" />
                </div>
                <div class="item">
                    <img src="https://cdn.vox-cdn.com/thumbor/H1B37U6msyVbRx-RIzdQWD6iKu0=/0x0:3840x2160/1200x800/filters:focal(1613x773:2227x1387)/cdn.vox-cdn.com/uploads/chorus_image/image/65144932/shutterstock_1486027769.0.jpg" alt="img2" width="500" />
                </div>
                <div class="item">
                    <img src="https://www.fodors.com/wp-content/uploads/2019/05/WhatNOTtodoinChicago__HERO_iStock-996188444.jpg" alt="img3" width="500" />
                </div>
            </div>
            <!-- Left and right controls -->
            <a class="left carousel-control" href="#myCarousel" data-slide="prev">
                <span class="glyphicon glyphicon-chevron-left"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="right carousel-control" href="#myCarousel" data-slide="next">
                <span class="glyphicon glyphicon-chevron-right"></span>
                <span class="sr-only">Next</span>
            </a>
        </div><hr />
    </div>
    <div>
        <div class="text">
            This Website is on the traffic crashes,
            
        <table width="100%" cellpadding="10" cellspacing="10" border="1" class="table table-bordered ">
            <tr>
                <th>Traffic Crashes</th>
                <th>Citizens of Chicago</th>
            </tr>
            <tr>
                <td class="text">
                   
                </td>
                <td class="text">
                    
                </td>
            </tr>
        </table><hr />
    </div>
</body>


